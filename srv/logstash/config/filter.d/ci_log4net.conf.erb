if [@type] == "ci_log4net" {
    grok {
        match => [ "@message", "%{NOTSPACE:level}\s+%{TIMESTAMP_ISO8601:datetime} %{NOTSPACE:thread} %{NOTSPACE:logger} %{GREEDYDATA:message}" ]
    }

    date {
        match => [ "datetime", "YYYY-MM-dd HH:mm:ss,SSS" ]
        timezone => "Europe/London"
    }
}
