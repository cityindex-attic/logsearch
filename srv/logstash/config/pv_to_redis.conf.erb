input {
    stdin {
        type => "<%= @args[:logstash_type] %>"
        format => "<%= @args[:logstash_format] || 'plain' %>"
    }
}

output {
    #stdout { debug => true debug_format => "json"}
    redis {
        host => "<%= ENV['APP_CONFIG_REDIS_IPADDRESS'] || '127.0.0.1'  %>"
        data_type => "list"
        key => "<%= ENV['APP_CONFIG_REDIS_KEY'] || 'logstash'  %>"
        batch => true
        batch_events => 250
    }
}
